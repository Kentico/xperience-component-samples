@using Kentico.Components.Web.Mvc.FormComponents;
@using Kentico.Components.Web.Mvc.Selectors;

@model ObjectSelector

@{
    var selectorId = Html.IdFor(m => m.SelectedValue).ToString();
}

@Html.Kentico().ObjectSelector(selectorId, Model.Properties.ObjectType)

<script>
(function () {
    var selector = document.getElementById("@selectorId");
    selector.dispatchEvent(new Event(selector.dataset.initializationEvent, {
        bubbles: true,
    }));
}());
</script>
