@using Kentico.Components.Web.Mvc.FormComponents;
@using Kentico.Components.Web.Mvc.Selectors.Internal;

@model ObjectSelector

@{
    var selectorId = $"objectSelector-{Guid.NewGuid()}";
}

@Html.ObjectSelector(selectorId)

<script>
(function () {
    var selector = document.getElementById("@selectorId");
    selector.dispatchEvent(new Event(selector.dataset.initializationEvent, {
        bubbles: true,
    }));
}());
</script>
